{{#each events}}
<div id="listEvents"> 
  <div id="generalInfo">
    <h3>general info</h3>
    <br>
    {{name}}
    <br>
    {{location.name}}
    <br>
    {{date}}
    <br>
    {{cost}}
    <br>
    {{ageOfEntrance}}
    <br>
    {{openingHours}}
    <br>
  </div>  

  <div id="lineUp">
      <h3>line up</h3>
      
      <br>
      {{artists}}
      <br>
      {{artistsSpotify}}
  </div>

  <div id="listEventsButtons">
    <button><a href="/list-events/delete/{{_id}}">Delete Event</a></button>
    <button><a href="/list-events/edit-event/{{_id}}">Edit event</a></button>
  </div>
</div>
 {{/each}}

