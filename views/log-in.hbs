<button  id="goBack">
    <a href="/"> Back to Homepage</a>
</button>

<div id="logIn">
  <h2>log in</h2>
</div>

<form id="logInForm" action="/log-in" method="POST">
    <input type="text" name="username" id="username" placeholder="type in username">
    <input type="password" name="password" id="password" placeholder="type in password">
    <button type="submit">log in</button>
</form>


{{#if message}}
<p>{{message}}</p>
{{/if}}